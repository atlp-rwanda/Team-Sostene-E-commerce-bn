{
  "name": "team-sostene-e-commerce-bn",
  "version": "1.0.0",
  "description": "E-commerce app bn team-sostene",
  "main": "index.js",
  "scripts": {
    "start": "babel-node build/index.js",
    "lint": "eslint \"src/**/*.{js,css,html,json,md}\"",
    "prettier": "eslint \"src/**/*.{js,css,html,json,md}\"",
    "lint:staged": "lint-staged",
    "lint:fix": "eslint --fix --ext .js .",
    "prettier:fix": "prettier --write \"src/**/*.{js,css,html,json,md}\"",
    "prettier:check": "prettier \"src/**/*.{js,css,html,json,md}\"",
    "prepare": "husky install",
    "test": "mocha --require @babel/register src/tests/**/*.js --recursive --exit --timeout 80000",
    "coverage": "nyc npm run test",
    "build": "npx babel src --out-dir build",
    "prestart": "npm run build"
  },
  "author": "team-sostene",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/atlp-rwanda/Team-Sostene-E-commerce-bn/issues"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.3",
    "coveralls": "^3.1.1",
    "eslint": "^8.1.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "prettier": "^2.8.4"
  },
  "dependencies": {
    "@babel/node": "^7.20.7",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.20.2",
    "@babel/register": "^7.21.0",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "chai": "^4.3.7",
    "chai-http": "^4.3.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "eslint-plugin-node": "^11.1.0",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "http-proxy-middleware": "^2.0.6",
    "joi": "^17.9.1",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4",
    "redis": "^4.6.5",
    "sequelize": "^6.29.1",
    "sinon": "^15.0.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.2",
    "uuid": "^9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atlp-rwanda/Team-Sostene-E-commerce-bn.git"
  },
  "keywords": [
    "E-commerce",
    "bn",
    "sostene"
  ],
  "homepage": "https://github.com/atlp-rwanda/Team-Sostene-E-commerce-bn#readme",
  "lint-staged": {
    "src/**/*.{js,css,html,json,md}": [
      "npm run prettier:fix",
      "npm run lint:fix",
      "npm run test",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run test git && update-index -g"
    }
  }
}
